<?xml version='1.0' encoding='utf-8'?>
<widget id="com.eLearning.HR*" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>HWTestApp</name>
    <description>
        OIC eLearning - Oman Insurance Academy
    </description>
    <author email="pankaj.sevlani@tameen.ae" href="http://tameen.ae">
        IT Team OIC
    </author>
    <content src="index.html" />
    <icon src="icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="deployment-target" value="7.0" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
</widget>
